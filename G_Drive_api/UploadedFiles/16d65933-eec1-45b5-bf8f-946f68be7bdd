package Entity;

import java.io.Serializable;

public class Student implements Serializable {
    String stdName;
    String rollno;
    String stdCnic;
    String fatherName;
    String fatherCnic;
    String address;
    String district;
    String tehsil;
    String dob;
    String status;
    String guardianName;
    String guardianCnic;
    String contact1, contact2;
    String qualification;
    String bloodGroup;
    String department;
    String admissiondate;

    //Default constructor
    public Student() {
    }

    public Student(String stdName, String rollno, String stdCnic, String fatherName, String fatherCnic,
                   String address, String district, String tehsil, String dob, String status, String guardianName,
                   String guardianCnic, String contact1, String contact2, String qualification, String bloodGroup,
                   String department,String admissiondate) {
        this.stdName = stdName;
        this.rollno = rollno;
        this.stdCnic = stdCnic;
        this.fatherName = fatherName;
        this.fatherCnic = fatherCnic;
        this.address = address;
        this.district = district;
        this.tehsil = tehsil;
        this.dob = dob;
        this.status = status;
        this.guardianName = guardianName;
        this.guardianCnic = guardianCnic;
        this.contact1 = contact1;
        this.contact2 = contact2;
        this.qualification = qualification;
        this.bloodGroup = bloodGroup;
        this.department = department;
        this.admissiondate=admissiondate;
    }
public void  setValues(Student s){
    this.stdName = s.stdName;
    this.rollno = s.rollno;
    this.stdCnic = s.stdCnic;
    this.fatherName = s.fatherName;
    this.fatherCnic = s.fatherCnic;
    this.address = s.address;
    this.district = s.district;
    this.tehsil = s.tehsil;
    this.dob = s.dob;
    this.status = s.status;
    this.guardianName = s.guardianName;
    this.guardianCnic = s.guardianCnic;
    this.contact1 = s.contact1;
    this.contact2 = s.contact2;
    this.qualification = s.qualification;
    this.bloodGroup = s.bloodGroup;
    this.department = s.department;
    this.admissiondate=s.admissiondate;
}

    public String getAdmissiondate() {
        return admissiondate;
    }

    public void setAdmissiondate(String admissiondate) {
        this.admissiondate = admissiondate;
    }

    public String getStdName() {
        return stdName;
    }

    public void setStdName(String stdName) {
        this.stdName = stdName;
    }

    public String getRollno() {
        return rollno;
    }

    public void setRollno(String rollno) {
        this.rollno = rollno;
    }

    public String getStdCnic() {
        return stdCnic;
    }

    public void setStdCnic(String stdCnic) {
        this.stdCnic = stdCnic;
    }

    public String getFatherName() {
        return fatherName;
    }

    public void setFatherName(String fatherName) {
        this.fatherName = fatherName;
    }

    public String getFatherCnic() {
        return fatherCnic;
    }

    public void setFatherCnic(String fatherCnic) {
        this.fatherCnic = fatherCnic;
    }

    public String getAddress() {
        return address;
    }

    public void setAddress(String address) {
        this.address = address;
    }

    public String getDistrict() {
        return district;
    }

    public void setDistrict(String district) {
        this.district = district;
    }

    public String getTehsil() {
        return tehsil;
    }

    public void setTehsil(String tehsil) {
        this.tehsil = tehsil;
    }

    public String getDob() {
        return dob;
    }

    public void setDob(String dob) {
        this.dob = dob;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getGuardianName() {
        return guardianName;
    }

    public void setGuardianName(String guardianName) {
        this.guardianName = guardianName;
    }

    public String getGuardianCnic() {
        return guardianCnic;
    }

    public void setGuardianCnic(String guardianCnic) {
        this.guardianCnic = guardianCnic;
    }

    public String getContact1() {
        return contact1;
    }

    public void setContact1(String contact1) {
        this.contact1 = contact1;
    }

    public String getContact2() {
        return contact2;
    }

    public void setContact2(String contact2) {
        this.contact2 = contact2;
    }

    public String getQualification() {
        return qualification;
    }

    public void setQualification(String qualification) {
        this.qualification = qualification;
    }

    public String getBloodGroup() {
        return bloodGroup;
    }

    public void setBloodGroup(String bloodGroup) {
        this.bloodGroup = bloodGroup;
    }

    public String getDepartment() {
        return department;
    }

    public void setDepartment(String department) {
        this.department = department;
    }
}