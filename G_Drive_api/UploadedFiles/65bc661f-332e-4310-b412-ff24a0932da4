package Entity;

import java.io.Serializable;

public class StudentClass implements Serializable {
    String  stdRollNo, classId,year,  boardRollNo,classStatus;

    public StudentClass(String stdRollNo, String classId, String year, String boardRollno, String classStatus) {
        this.stdRollNo = stdRollNo;
        this.classId = classId;
        this.year = year;
        this.boardRollNo = boardRollno;
        this.classStatus = classStatus;
    }

    public StudentClass() {
    }

    public String getStdRollNo() {
        return stdRollNo;
    }

    public void setStdRollNo(String stdRollNo) {
        this.stdRollNo = stdRollNo;
    }

    public String getClassId() {
        return classId;
    }

    public void setClassId(String classId) {
        this.classId = classId;
    }

    public String getYear() {
        return year;
    }

    public void setYear(String year) {
        this.year = year;
    }

    public String getBoardRollno() {
        return boardRollNo;
    }

    public void setBoardRollno(String boardRollno) {
        this.boardRollNo = boardRollno;
    }

    public String getClassStatus() {
        return classStatus;
    }

    public void setClassStatus(String classStatus) {
        this.classStatus = classStatus;
    }
}
